    {
      "parameters": {
        "jsCode": "// Get raw command output\nconst commandResult = $('Execute Command').first().json;\n\n// Parse JSON string from stdout\nlet shipData;\ntry {\n  shipData = JSON.parse(commandResult.stdout);\n} catch (parseError) {\n  // Fallback if parsing fails\n  return {\n    chatid: $('Telegram Trigger').first().json.message.chat.id,\n    text: `Error parsing ship data: ${parseError.message}\\n\\n${commandResult.stdout.substring(0, 500)}`,\n    parsemode: 'Markdown'\n  };\n}\n\n// Format Telegram message\nif (!shipData.success) {\n  return {\n    chatid: $('Telegram Trigger').first().json.message.chat.id,\n    text: `${shipData.message} Ship lookup failed.`,\n    parsemode: 'Markdown'\n  };\n}\n\nconst data = shipData.data;\nlet message = `\n**${data.name || 'Unknown Ship'}**\n\n**IMO:** ${data.imo || 'NA'}\n**Flag:** ${data.flag || 'NA'}\n**Type:** ${data.type || 'NA'}\n**Tonnage:** ${data.grossTonnage || 'NA'}\n**Built:** ${data.buildDate || 'NA'}\n\n**Owner:** ${data.ownerName || 'NA'}\n${data.ownerDetails ? `**Address:** ${data.ownerDetails.address || 'NA'}` : ''}\n${data.history ? `**Name History:** ${data.history.names?.map(n => n).join(', ') || 'NA'}` : ''}\n**Status:** ${data.ownerDetails?.status || 'Active'}\n`;\n\nreturn {\n  chatid: $('Telegram Trigger').first().json.message.chat.id,\n  text: message,\n  parsemode: 'Markdown',\n  disable_web_page_preview: true\n};\n"
      },
